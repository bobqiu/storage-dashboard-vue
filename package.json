{
  "name": "storage-dashboard",
  "version": "0.2.0",
  "description": "CWN Dashboard",
  "author": "@baishancloud.com",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --port 80",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "aws-sdk": "2.33.0",
    "axios": "^0.16.2",
    "bluebird": "^3.5.1",
    "bytes": "^3.0.0",
    "clipboard": "^1.7.1",
    "core-js": "^2.5.3",
    "echarts": "^4.0.4",
    "es6-promise": "^4.1.1",
    "eslint-plugin-node": "^5.2.1",
    "file-saver": "^1.3.3",
    "iview-bsc": "2.10.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.21",
    "querystring-browser": "^1.0.4",
    "text-encoding": "^0.6.4",
    "vue": "^2.5.17",
    "vue-color": "^2.4.3",
    "vue-echarts": "^3.0.9",
    "vue-i18n": "^5.0.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-prettier": "^3.0.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "prerender-spa-plugin": "^3.4.0",
    "style-resources-loader": "^1.2.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "radix": 0,
      "no-return-assign": 0,
      "no-empty-function": 0,
      "no-param-reassign": 0,
      "no-extra-boolean-cast": 0,
      "no-new": 0,
      "no-console": 0,
      "array-callback-return": 0,
      "complexity": 0,
      "no-empty": 0,
      "no-undefined": 0,
      "semi": [
        "error",
        "never"
      ],
      "no-unexpected-multiline": 0,
      "no-multiple-empty-lines": 0,
      "no-sequences": 0,
      "no-control-regex": 0,
      "no-mixed-spaces-and-tabs": [
        "error",
        "smart-tabs"
      ],
      "vue/valid-v-model": 0,
      "vue/no-parsing-error": 0,
      "arrow-parens": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "generator-star-spacing": 0,
      "prefer-promise-reject-errors": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "_": false,
      "$t": false,
      "$": false
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
